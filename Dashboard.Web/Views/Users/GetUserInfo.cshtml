
@model Dashboard.Web.ViewModels.UserInfoModel
<div class="container">
    <div class="row">
        
        <div class="col-md-12">
            <h2 class="text-center">Claims</h2>
            <table class="table">
                <thead>
                    <tr><th>Claim Type</th><th>Claim Value</th></tr>
                </thead>
                <tbody>

                    @foreach (var claim in Model.Claims)
                    {
                        <tr>
                            <td><b>@claim.Type</b></td>
                            <td>@claim.Value</td>
                        </tr>
                    }

                </tbody>
            </table>
        </div>
    </div>
</div>